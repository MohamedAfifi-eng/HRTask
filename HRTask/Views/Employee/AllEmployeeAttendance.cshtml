@model List<EmployeeAttendance>
@{
    ViewData["Title"] = "AllEmployeeAttendance";
}

<section class="container">
    <div class="row text-center">
        <h1>AllEmployeeAttendance</h1>
        <table class="table table-bordered table-hover table-striped">
            <thead>
                <tr>
                    <th>#</th>
                    <th>Name</th>
                    <th>Arrive Time</th>
                    <th>Leave Time</th>
                </tr>
            </thead>
            <tbody>
                @for (var i = 0; i < Model.Count; i++)
                {
                    
                
                    <tr>
                        <th>@(i+1)</th>
                        <th>@Model[i].Employee.Name</th>
                        <th>@Model[i].TimeOfAttendance</th>
                        <th>@Model[i].TimeOfLeave</th>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</section>



